{
    "name": "amethyst/work",
    "keywords": ["laravel", "amethyst", "amethyst-work", "work"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "amethyst/common": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "3.*",
        "amethyst/user": "0.1.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\WorkServiceProvider"
            ]
        }
    }
}
